---
import Head from "@components/Head.astro";
import Header from "@components/Header.astro";
import Footer from "@components/Footer.astro";
import { SITE } from "@src/config.ts";
---

<html lang="en">
  <Head
    title={SITE.title}
    description={SITE.desc}
    image={SITE.ogImage}
    url={SITE.website}
    author={SITE.author}
  />
  <body>
    <Header/>
      <slot />
    <Footer />
  </body>
</html>
